{
  "name": "gulp-pylint",
  "version": "0.0.1",
  "description": "Run Pylint in Gulp",
  "main": "lib/index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint src/*.js",
    "test": "mocha  --timeout 5000 --compilers js:babel-core/register",
    "build": "babel src/index.js --out-file lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benmcp/gulp-pylint.git"
  },
  "keywords": [
    "json",
    "object"
  ],
  "author": "Ben McPhail",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benmcp/gulp-pylint/issues"
  },
  "homepage": "https://github.com/benmcp/gulp-pylint#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.5",
    "mocha": "^4.0.1",
    "object-depth": "^0.0.3",
    "shelljs": "^0.8.2",
    "through2": "^2.0.3"
  }
